print (Input three small numbers ) ;
read M[8];
read M[9];
read M[10];
print (Input data are )  M[8] ( )  M[9] ( )  M[10]  M[6] (\n);
load R0 M[8];
load R1  5;
sub R0 R0 R1;
store M[8] R0;
load R0  3;
load R1 M[9];
add R0 R0 R1;
store M[9] R0;
load R0  2;
load R1 M[10];
add R0 R0 R1;
store M[10] R0;
load R0 M[9];
load R1 M[10];
add R0 R0 R1;
load R1 M[8];
add R0 R0 R1;
store M[11] R0;
print (Modified input are )  M[8] ( )  M[9] ( )  M[10] (\n) ;
print (Final result is )  M[11] (\n) ;
